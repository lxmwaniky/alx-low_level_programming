CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm -f

# make all i.e. makes all files executable
all : $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

# deletes any temporary files and executable
clean :
	$(RM) *~ $(NAME)

# deletes object files
oclean :
	$(RM) $(OBJ)

# delets all (temp files, executables & object files)
fclean : clean oclean

# deletes all and recompiles all source files
re : oclean all
